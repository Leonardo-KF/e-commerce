        <section class="Cadastro">
            <div>
                <label class="nome_completo" for="nome">Nome completo: </label>
                <input type="text" name="nome_completo" id="nome_completo" placeholder="Nome completo" required>
            </div>
            <div>
                <label class="nome_login" for="nome">Login: </label>
                <input type="text" name="nome_login" id="nome_login" placeholder="Login do usuário" required>
            </div>
            <div>
                <label class="senha" for="senha">senha: </label>
                <input type="password" name="senha" id="senha" placeholder="Senha de acesso" required>
            </div>
            <div>
                <label class="email" for="email">E-mail: </label>
                <input type="text" name="email" id="email" placeholder="Seu melhor e-mail" required>
            </div>
            <div>
                <label class="telefone" for="telefone">Telefone: </label>
                <input type="tel" name="telefone" id="telefone" placeholder="Seu número de telefone" required>
            </div>
            <button type="submit">Enviar informações</button>
        </section>
